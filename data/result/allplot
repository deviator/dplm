#!/bin/bash

gnuplot -e "fname='exp_16_find_4_rnd'" plot
gnuplot -e "fname='exp_16_rnd_auto'" plot
gnuplot -e "fname='exp_16_serial'" plot

gnuplot -e "fname='exp_33_find_4_rnd'" plot
gnuplot -e "fname='exp_33_rnd_auto'" plot
gnuplot -e "fname='exp_33_serial'" plot

gnuplot -e "fname='exp_50_find_4_rnd'" plot
gnuplot -e "fname='exp_50_rnd_auto'" plot
gnuplot -e "fname='exp_50_serial'" plot

gnuplot -e "n='all_find_4';f1='exp_16_find_4_rnd';f2='exp_33_find_4_rnd';f3='exp_50_find_4_rnd'" plot3
gnuplot -e "n='all_rnd_auto';f1='exp_16_rnd_auto';f2='exp_33_rnd_auto';f3='exp_50_rnd_auto'" plot3 
gnuplot -e "n='all_serial';f1='exp_16_serial';f2='exp_33_serial';f3='exp_50_serial'" plot3

gnuplot -e "n='all_16';f1='exp_16_find_4_rnd';f2='exp_16_rnd_auto';f3='exp_16_serial'" plot3
gnuplot -e "n='all_33';f1='exp_33_find_4_rnd';f2='exp_33_rnd_auto';f3='exp_33_serial'" plot3
gnuplot -e "n='all_50';f1='exp_50_find_4_rnd';f2='exp_50_rnd_auto';f3='exp_50_serial'" plot3
